{
  "name": "planku",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon run",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix --ignore-pattern \"api/**\" ./",
    "test": "jest ./",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed:remove": "sequelize-cli db:seed:undo:all",
    "build:ncc": "ncc build app.js -o api",
    "build:min": "uglifyjs api/index.js -o api/index.js",
    "build:git": "git add api/* && git commit -m \"deploy staging: $(date)\" && git push origin",
    "build:staging": "npm run build:ncc && npm run build:min && npm run build:git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "uglify-js": "^3.17.4"
  }
}
